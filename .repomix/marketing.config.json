{
  "output": {
    "filePath": "marketing.repomix.txt",
    "style": "markdown",
    "headerText": "front.js - Marketing Context (Features & Examples)",
    "removeComments": true,
    "showLineNumbers": false
  },
  "include": [
    "README.md",
    "wiki/PRD.md",
    "wiki/FAQ.md",
    "wiki/API.md",
    "wiki/TRANSLATIONS.md",
    "examples/**/*.html",
    "examples/**/*.js",
    "examples/README.md",
    "package.json"
  ],
  "ignore": {
    "useGitignore": true,
    "useDefaultPatterns": true,
    "customPatterns": [
      "src/**/*",
      "tests/**/*",
      "docs/**/*",
      "community/**/*",
      "*.config.js",
      "*.test.js",
      "CONTRIBUTING.md",
      "CLAUDE.md"
    ]
  }
}
